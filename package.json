  },
  "author": "Thomas Collardeau",
  "license": "MIT",
  "dependencies": {
    "awesome-typescript-loader": "^4.0.1",
    "source-map-loader": "^0.2.3",
    "typescript": "^2.7.2"
  },
  "devDependencies": {
    "@types/jest": "^22.2.1",
    "@types/react": "^16.1.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "jest": "^22.4.2",
    "react": "^16.3.0",
    "react-test-renderer": "^16.3.0",
    "ts-jest": "^22.4.2",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
